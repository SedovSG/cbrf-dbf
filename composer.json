{
    "name": "sedovsg/cbrf-dbf",
    "description": "Библиотека для работы с базой данных DBF Центрального банка РФ",
    "keywords": ["SedovSG", "cbrf", "dbf"],
    "type": "library",
    "homepage": "http://www.omnisoft.pro/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sedov Stas",
            "email": "SedovSG@yandex.ru",
            "homepage": "http://www.sedovsg.me/",
            "role": "Developer"
        }
    ],
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpunit/php-invoker": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Cbrf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cbrf\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
